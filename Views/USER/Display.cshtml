@model IEnumerable<BasicWebApp.Models.USER>

@{
    ViewBag.Title = "Display";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Display</h2>

<div>
    <table>
        <thead>
            <tr>

                <th scope="col">Name</th>
                <th scope="col">Password</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr>
                    <td style="text-align: left;">@i.userName</td>
                    <td style="text-align: right;">@i.password</td>
                </tr>
            }
        </tbody>
    </table>
</div>